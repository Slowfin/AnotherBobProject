function scrBossSpawn(){
	
		vsp += grv
		scrPlayerCollision()
		y += vsp
		x += hsp * sp
		
		
		sprite_index  = sprBossSpawn
		objCamera.shakePower = 2
		objCamera.isShaking = true
}